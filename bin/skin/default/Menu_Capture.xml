<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Window bktrans="true" size="240,160" roundcorner="2,2" >
<Font name="微软雅黑" size="14" bold="false" italic="false" />
<Font name="微软雅黑" size="14" bold="true" italic="false" />
<VerticalLayout bkcolor="#FF000000" bordersize="1" bordercolor="#FF1B1B1B"> 
	<List name="menu_capture" 
		itemendellipsis="true"
		header="hidden"
		itemhotbkcolor="#FF1B1B1B" 
		itemselectedbkcolor="#FF151C2C"
		itemtextcolor="#FFFFFFFF"
		>
		<ListContainerElement name="menu_Interfaces" height="25" padding="0,2,0,0" >
			<HorizontalLayout>
				<Control
					padding="5,4,5,0"
					width="16"
					height="16"
					mouse="false"
					bkimage="file='menu_fav.png'"
					/>
				
				<Label text="Interfaces...               Ctrl+I"  mouse="false" textcolor="0XFFFFFFFF" />
			</HorizontalLayout>
		</ListContainerElement>
		<ListContainerElement name="menu_Options" height="25" padding="0,2,0,0">
			<HorizontalLayout>
				<Control
					padding="5,4,5,0"
					width="16"
					height="16"
					mouse="false"
					bkimage="file='menu_fav.png'"
				/>
				<Label text="Options...                  Ctrl+K" mouse="false" textcolor="0XFFFFFFFF" />
			</HorizontalLayout>
		</ListContainerElement>
		<ListContainerElement name="menu_Start" height="25">
			<HorizontalLayout>
				<Control
					padding="5,4,5,0"
					mouse="false"
					width="16"
					height="16"
					bkimage="file='menu_fav.png'"
				/>
				<Label text="Start                       Ctrl+E" mouse="false" textcolor="0XFFFFFFFF"/>
			</HorizontalLayout>
		</ListContainerElement>
		<ListContainerElement name="menu_Stop" height="25">
			<HorizontalLayout>
				<Control
					padding="5,4,5,0"
					mouse="false"
					width="16"
					height="16"
					bkimage="file='menu_fav.png'"
				/>
				<Label text="Stop                        Ctrl+E" mouse="false" textcolor="0XFFFFFFFF"/>
			</HorizontalLayout>
		</ListContainerElement>
		<ListContainerElement name="menu_restart" height="25">
			<HorizontalLayout>
				<Control
					padding="5,4,5,0"
					mouse="false"
					width="16"
					height="16"
					bkimage="file='menu_fav.png'"
				/>
				<Label text="Restart                     Ctrl+R" mouse="false" textcolor="0XFFFFFFFF" />
			</HorizontalLayout>
		</ListContainerElement>
		<ListContainerElement name="menu_captureFilter" height="25">
			<HorizontalLayout>
				<Control
					padding="5,4,5,0"
					mouse="false"
					width="16"
					height="16"
					bkimage="file='menu_fav.png'"
				/>
				<Label text="Capture Filters...          Ctrl+R" mouse="false" textcolor="0XFFFFFFFF"/>
			</HorizontalLayout>
		</ListContainerElement>
		<ListContainerElement name="menu_refresh" height="25">
			<HorizontalLayout>
				<Control
					padding="5,4,5,0"
					mouse="false"
					width="16"
					height="16"
					bkimage="file='menu_fav.png'"
				/>
				<Label text="Refresh Interfaces          Ctrl+R" mouse="false" textcolor="0XFFFFFFFF"/>
			</HorizontalLayout>
		</ListContainerElement>
	</List>
</VerticalLayout>
</Window>